{
  "name": "Deezer/test-tech",
  "description": "Deezer Test Tech",
  "homepage": "https://github.com/albert-barreto/deezer",
  "require": {
      "php": "^7.1",
      "ext-PDO": "*",
      "slim/slim": "3.9.2",
      "slim/php-view": "^2.0",
      "monolog/monolog": "^1.23",
      "slim/twig-view": "^2.4",
      "slim/pdo": "~1.10",
      "respect/validation": "^1.1",
      "guzzlehttp/guzzle": "~6.0",
      "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0"
  },
  "autoload": {
    "psr-4": {
      "Deezer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deezer\\": "src/"
    }
  },
  "config": {
    "process-timeout" : 0
  },
  "scripts": {
      "start": "php -S localhost:8787 -t public"
  }
}
